msgid "Search"
msgstr "Tìm kiếm"

#: ../libnautilus-private/nautilus-query.c:210
#, c-format
msgid "Search for “%s”"
msgstr "Tìm “%s”"

#: ../libnautilus-private/nautilus-search-engine.c:189
