Package: nautilus
Architecture: any
XCBS-EOS-AppId: org.gnome.Nautilus
Depends: ${shlibs:Depends},
         ${misc:Depends},
         nautilus-data (>= ${gnome:Version}),
         nautilus-data (<< ${gnome:NextVersion}),
         shared-mime-info (>= 0.50),
         desktop-file-utils (>= 0.7),
         gvfs (>= 1.3.2),
         libglib2.0-data,
         gsettings-desktop-schemas (>= 3.8.0)
Recommends: librsvg2-common,
            gvfs-backends,
            gnome-disk-utility,
            gnome-sushi
